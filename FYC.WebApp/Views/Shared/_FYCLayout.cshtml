            <!-- Charts -->
            <script>

                // Sales this month chart
                var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"]

                var data = [
                    { y: '2017-01', a: 210, b: 90 },
                    { y: '2017-02', a: 165, b: 75 },
                    { y: '2017-03', a: 250, b: 50 },
                    { y: '2017-04', a: 375, b: 160 },
                    { y: '2017-05', a: 280, b: 65 },
                    { y: '2017-06', a: 390, b: 170 },
                    { y: '2017-07', a: 400, b: 75 },
                    { y: '2017-08', a: 315, b: 175 },
                    { y: '2017-09', a: 220, b: 85 },
                    { y: '2017-11', a: 345, b: 85 },
                    { y: '2017-12', a: 360, b: 95 }
                ],
                    config = {
                        data: data,
                        xkey: 'y',
                        ykeys: ['a', 'b'],
                        labels: ['Total Donors', 'Total Beneficiary'],
                        fillOpacity: 0.6,
                        hideHover: 'auto',
                        behaveLikeLine: true,
                        resize: true,
                        pointFillColors: ['#ffffff'],
                        pointStrokeColors: ['#2da9e9'],
                        lineColors: ['#2da9e9', '#0ec8a2'],
                        barColors: ['#2da9e9', '#0ec8a2'],
                        xLabelFormat: function (x) { // <--- x.getMonth() returns valid index
                            var month = months[x.getMonth()];
                            return month;
                        },
                        dateFormat: function (x) {
                            var month = months[new Date(x).getMonth()];
                            return month;
                        }
                    };

                config.element = 'area-chart';
                Morris.Area(config);
            </script>
        </div>


