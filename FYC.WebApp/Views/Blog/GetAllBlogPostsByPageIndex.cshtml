@model List<FYC.DataAccess.Entities.Category>
@{
    ViewBag.Title = "GetAllBlogPostsByPageIndex";
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.CategoryId</td>
                            <td>@item.CategoryName</td>
                        </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>
</div>


